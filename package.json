    "fetch-mock": "9.11.0",
    "fetch-mock-jest": "1.5.1",
    "jest": "28.1.3",
    "kill-port": "^2.0.1",
    "nodemon": "2.0.19",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1"
  }
